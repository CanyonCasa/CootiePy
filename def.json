{
    "cfg": {
        "ack": false,
        "quiet": false,
        "verbose": true
    },
    "xjobs": [
        { "id": "report", "at": "0 */3 10 * * *", "evt":{"tag": "status", "cmd": "status", "ping": "reporting..." }},
        { "id": "alt", "at": "0 */2 10 * * *", "evt":{"tag": "status", "cmd": "status", "ping": "alt report..." }}
    ],
    "io":[
        {
            "driver": "OneWire",
            "debug": true,
            "name": "1wire",
            "params": {"pin": "D1"}
        },
        {
            "driver": "Analog",
            "name": "analog",
            "debug": true
        },
        {
            "driver": "Digital",
            "name": "digital",
            "debug": true
        },
        {
            "driver": "PWM",
            "name": "pwm",
            "debug": true
        },
        {
            "interface": "1wire",
            "desc": "Outside temperature measurement (F)", 
            "name": "tOutside",
            "sn": "28 69 2F 2C 0C 32 20 9A",
            "params": {"units": "F", "resolution": 8}
        },
        {
            "interface": "1wire",
            "desc": "Inside temperature measurement (F)",
            "name": "tInside",
            "sn": "28-5445ff000000"
        },
        {
            "interface": "1wire",
            "desc": "West Sprinkler Port",
            "name": "west",
            "sn": "29 EE D2 02 00 00 00 C3"
        },
        {
            "interface": "1wire",
            "desc": "South Sprinkler Port",
            "name": "south",
            "sn": "29 0B B7 19 00 00 00 6E"
        },
        {
            "interface": "1wire",
            "desc": "Cental Sprinkler Port",
            "name": "central",
            "sn": "29 E5 D2 02 00 00 00 3B"
        },
        {
            "interface": "1wire",
            "desc": "East Sprinkler Port",
            "name": "east",
            "sn": "29 F1 D2 02 00 00 00 BC"
        },
        {
            "interface": "1wire",
            "desc": "Cental Temp",
            "name": "tcentral",
            "sn": "28 9E A0 16 A8 01 3C B4"
        },
        {
            "interface": "analog",
            "desc": "Loop Pressure",
            "name": "pressure",
            "params": {"pin":"A0", "range":3.3, "bias": 0, "units":"V"}
        },
        {
            "interface": "digital",
            "desc": "Test output",
            "name": "test",
            "params": {"pin":"D1", "term":"up", "init": 1}
        },
        {
            "interface": "pwm",
            "desc": "Fan driver",
            "name": "fan",
            "params": {"pin":"D2", "dc":0, "freq": 1000}
        }
    ]
}